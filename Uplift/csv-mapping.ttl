		rr:objectMap    [
	        rrf:functionCall [
	 			rrf:function <#ExtractDate> ;
	 			rrf:parameterBindings (
	 				[ rr:column "SDATE" ]
	 			) ;
	 		] ; 
	 	]
	] ;
	.
	
	
